[
  {
    "number": 5968,
    "text": "HAS THERE COME unto thee the tiding of the Overshadowing Event?'",
    "numberInSurah": 1,
    "juz": 30,
    "manzil": 7,
    "page": 592,
    "ruku": 530,
    "hizbQuarter": 237,
    "sajda": false
  },
  {
    "number": 5969,
    "text": "Some faces will on that Day be downcast,",
    "numberInSurah": 2,
    "juz": 30,
    "manzil": 7,
    "page": 592,
    "ruku": 530,
    "hizbQuarter": 237,
    "sajda": false
  },
  {
    "number": 5970,
    "text": "toiling [under burdens of sin], worn out [by fear],",
    "numberInSurah": 3,
    "juz": 30,
    "manzil": 7,
    "page": 592,
    "ruku": 530,
    "hizbQuarter": 237,
    "sajda": false
  },
  {
    "number": 5971,
    "text": "about to enter a glowing fire,",
    "numberInSurah": 4,
    "juz": 30,
    "manzil": 7,
    "page": 592,
    "ruku": 530,
    "hizbQuarter": 237,
    "sajda": false
  },
  {
    "number": 5972,
    "text": "given to drink from a boiling spring.",
    "numberInSurah": 5,
    "juz": 30,
    "manzil": 7,
    "page": 592,
    "ruku": 530,
    "hizbQuarter": 237,
    "sajda": false
  },
  {
    "number": 5973,
    "text": "No food for them save the bitterness of dry thorns,",
    "numberInSurah": 6,
    "juz": 30,
    "manzil": 7,
    "page": 592,
    "ruku": 530,
    "hizbQuarter": 237,
    "sajda": false
  },
  {
    "number": 5974,
    "text": "which gives no strength and neither stills hunger.",
    "numberInSurah": 7,
    "juz": 30,
    "manzil": 7,
    "page": 592,
    "ruku": 530,
    "hizbQuarter": 237,
    "sajda": false
  },
  {
    "number": 5975,
    "text": "[And] some faces will on that Day shine with bliss,",
    "numberInSurah": 8,
    "juz": 30,
    "manzil": 7,
    "page": 592,
    "ruku": 530,
    "hizbQuarter": 237,
    "sajda": false
  },
  {
    "number": 5976,
    "text": "well-pleased with [the fruit of] their striving,",
    "numberInSurah": 9,
    "juz": 30,
    "manzil": 7,
    "page": 592,
    "ruku": 530,
    "hizbQuarter": 237,
    "sajda": false
  },
  {
    "number": 5977,
    "text": "in a garden sublime,",
    "numberInSurah": 10,
    "juz": 30,
    "manzil": 7,
    "page": 592,
    "ruku": 530,
    "hizbQuarter": 237,
    "sajda": false
  },
  {
    "number": 5978,
    "text": "wherein thou wilt hear no empty talk.",
