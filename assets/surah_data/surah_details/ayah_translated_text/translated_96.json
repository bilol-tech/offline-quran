    "page": 597,
    "ruku": 538,
    "hizbQuarter": 239,
    "sajda": false
  },
  {
    "number": 6115,
    "text": "HAST THOU ever considered him who tries to prevent",
    "numberInSurah": 9,
    "juz": 30,
    "manzil": 7,
    "page": 597,
    "ruku": 538,
    "hizbQuarter": 239,
    "sajda": false
  },
  {
